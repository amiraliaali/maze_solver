        {
            "name": "Python: Original Maze",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/maze.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Value Iteration",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/algorithms/value_iteration.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/scripts"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Policy Iteration",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/algorithms/policy_iteration.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/scripts"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
